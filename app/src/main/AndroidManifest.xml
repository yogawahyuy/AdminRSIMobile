<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rsip.adminrsimoblie">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RecyclerView.ListKeluhanActivity"
            android:label="Keluhan"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".View.DaftarKeluhanActivity" />
        <activity android:name=".View.EditDokterActivity" />
        <activity android:name=".View.DetailDokterActivity" />
        <activity android:name=".View.TambahDokterActivity" />
        <activity
            android:name=".View.InfoDokterActivity"
            android:label="Info Dokter"
            android:theme="@style/AppTheme" />
        <activity android:name=".View.MenuInfoDokterActivity" />
        <activity android:name=".View.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>